<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class name="ar.com.scriptorum.beans.GroupMember" table="group_members">
        <id name="id" column="id" type="long">
        	<generator class="native"/>
        </id>
		<many-to-one name="group" class="Group" column="group_id" lazy="false" />
		<many-to-one name="carpooler" class="Carpooler" column="carpooler_id" lazy="false" />
		<property name="fecha" column="fecha" type="java.util.Date"/>
    </class>
</hibernate-mapping>